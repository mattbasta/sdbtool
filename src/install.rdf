<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>sdbizo@bizo.com</em:id>
    <em:type>2</em:type>
    <em:version>__VERSION__.__BUILD__</em:version>

    <!-- Target Application this extension can install into,
         with minimum and maximum supported versions. -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.5</em:minVersion>
        <em:maxVersion>4.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>SDBizo</em:name>
    <em:description>Mozilla Firefox extension for interacting with Amazon SimpleDB (SDB).</em:description>
    <em:creator>Donnie Flood at Bizo, Inc (http://www.bizo.com)</em:creator>
    <em:contributor>Matt Basta at Serverboy Software (http://serverboy.net/)</em:contributor>
    <em:homepageURL>http://www.bizo.com</em:homepageURL>
  </Description>
</RDF>
